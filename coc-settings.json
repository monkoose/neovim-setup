{
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.enableMessage": "jump",
  "suggest.minTriggerInputLength": 2,
  "session.directory": "~/.local/share/nvim/sessions",
  "list.source.grep.args": ["-L"],
  "session.saveOnVimLeave": false,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "coc.preferences.previewAutoClose": false,
  "coc.preferences.formatOnSaveFiletypes": ["haskell"],
  "coc.preferences.extensionUpdateCheck": "weekly",
  "snippets.autoTrigger": false,
  "snippets.ultisnips.directories": [],
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        "stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    }
  },
  "python.jediEnabled": false
}

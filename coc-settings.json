{
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.enableMessage": "jump",
  "suggest.minTriggerInputLength": 2,
  "yank.highlight.duration": 400,
  "yank.enableCompletion": false,
  "coc.preferences.previewAutoClose": false,
  "coc.preferences.formatOnSaveFiletypes": ["svelte", "rust"],
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.colorSupport": false,
  "snippets.autoTrigger": false,
  "snippets.ultisnips.enable": false,
  "sh.highlightParsingErrors": false,
  "html.suggest.angular1": false,
  "html.suggest.ionic": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "svelte": ["html", "svelte"]
  },
  "languageserver": {
    "lua": {
      "cwd": "/usr/share/lua-language-server",
      "command": "/usr/bin/lua-language-server",
      "args": ["-E", "-e", "LANG=en", "/usr/share/lua-language-server/main.lua"],
      "filetypes": ["lua"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
        }
      }
    }
  },
  "python.jediEnabled": false
}
